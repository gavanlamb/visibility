{
  "service": {
    "host": "localhost",
    "port": 8081,
    "metrics": {
      "enabled": true,
      "collectDefaultMetrics": true,
      "requestDurationBuckets": [1, 5, 7, 9, 11, 13, 15, 20, 30]
    },
    "logging": {
      "level": "debug",
      "console": {
        "json": true,
        "colorize": false
      }
    }
  },
  "rendering": {
    "mode": "default",
    "verboseLogging": true,
    "args": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-dev-shm-usage",
      "--disable-accelerated-2d-canvas",
      "--disable-gpu",
      "--window-size=1280x758"
    ],
    "ignoresHttpsErrors": true,
    "timezone": "Australia/Sydney",
    "width": 1000,
    "maxWidth": 3080,
    "height": 500,
    "maxHeight": 3000,
    "deviceScaleFactor": 4,
    "maxDeviceScaleFactor": 8,
    "pageZoomLevel": 1
  }
}
